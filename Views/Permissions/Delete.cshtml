@model MedicineStock.Models.Permission

@{
    ViewData["Title"] = "Delete";
    ViewBag.Menu = "Permission";
}

<h1>Bạn có chắc chắn muốn xoá quyền này?</h1>

@await Html.PartialAsync("_DetailsPermission", Model)
<form asp-action="Delete">
    <input type="hidden" asp-for="PermissionId" />
    <button type="submit" class="btn btn-danger">Xoá</button>
    <button asp-action="Index" class="btn btn-primary">Trở về</button>
</form>